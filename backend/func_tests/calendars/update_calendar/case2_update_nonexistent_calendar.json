{
  "name": "Update nonexistent calendar (should fail)",
  "description": "Intentar actualizar un calendario que no existe",
  "setup": {
    "users": [
      {
        "id": 1,
        "name": "Test User",
        "phone": "+34600000000"
      }
    ]
  },
  "request": {
    "method": "PUT",
    "endpoint": "/api/v1/calendars/9999?current_user_id=1",
    "body": {
      "name": "Calendario Inexistente"
    }
  },
  "expected_response": {
    "status_code": 404
  },
  "expected": {
    "status_code": 404,
    "body": {
      "detail": "Calendar not found"
    }
  }
}
