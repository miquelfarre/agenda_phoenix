{
  "name": "Create user without contact (Instagram user)",
  "description": "Crear un usuario de Instagram sin contacto asociado",
  "setup": {
    "users": []
  },
  "request": {
    "method": "POST",
    "endpoint": "/api/v1/users",
    "body": {
      "instagram_name": "instagram_user",
      "auth_provider": "instagram",
      "auth_id": "ig_12345678",
      "profile_picture": "https://example.com/profile.jpg"
    }
  },
  "expected_response": {
    "status_code": 201
  },
  "expected": {
    "status_code": 201,
    "body": {
      "name": null,
      "instagram_name": "instagram_user",
      "phone": null,
      "auth_provider": "instagram",
      "auth_id": "{{ID}}",
      "is_public": false,
      "is_admin": false,
      "profile_picture": "https://example.com/profile.jpg",
      "id": "{{ID}}",
      "contact_id": null,
      "last_login": null,
      "created_at": "{{TIMESTAMP}}",
      "updated_at": "{{TIMESTAMP}}"
    }
  }
}
