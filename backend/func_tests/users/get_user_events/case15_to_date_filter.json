{
  "description": "Filter events up to a specific date",
  "setup": {
    "users": [
      {"id": 1, "name": "Alice", "phone": "+1234567890"}
    ],
    "events": [
      {"id": 1, "name": "Past Event", "start_date": "2025-11-01T10:00:00", "owner_id": 1},
      {"id": 2, "name": "Event 1", "start_date": "2025-12-01T10:00:00", "owner_id": 1},
      {"id": 3, "name": "Event 2", "start_date": "2025-12-15T10:00:00", "owner_id": 1},
      {"id": 4, "name": "Future Event", "start_date": "2026-01-01T10:00:00", "owner_id": 1}
    ]
  },
  "request": {
    "method": "GET",
    "endpoint": "/users/1/events?to_date=2025-12-31"
  },
  "expected": {
    "status_code": 200,
    "note": "Should return events with id=1,2,3 (up to 2025-12-31), NOT id=4"
  }
}
