{
  "name": "Delete non-existent contact (should fail)",
  "description": "Intentar eliminar un contacto que no existe",
  "setup": {
    "users": [
      {
        "id": 1,
        "name": "Alice",
        "phone": "+34900000001"
      }
    ]
  },
  "request": {
    "method": "DELETE",
    "endpoint": "/contacts/9999?current_user_id=1"
  },
  "expected": {
    "status_code": 404,
    "body": {
      "detail": "Contact not found"
    }
  }
}
