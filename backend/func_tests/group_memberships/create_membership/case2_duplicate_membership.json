{
  "description": "Create duplicate group membership returns 400",
  "setup": {
    "users": [
      {"id": 1, "name": "Alice", "phone": "+1234567890"}
    ],
    "groups": [
      {"id": 1, "name": "Team A", "created_by": 1}
    ],
    "group_memberships": [
      {"id": 1, "group_id": 1, "user_id": 1}
    ]
  },
  "request": {
    "method": "POST",
    "endpoint": "/group_memberships",
    "body": {
      "group_id": 1,
      "user_id": 1
    }
  },
  "expected": {
    "status_code": 400,
    "body_contains": ["User is already a member of this group"]
  }
}
