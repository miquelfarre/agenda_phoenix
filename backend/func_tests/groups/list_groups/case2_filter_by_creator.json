{
  "description": "List groups filtered by creator",
  "setup": {
    "users": [
      {"id": 1, "name": "Alice", "phone": "+1234567890"},
      {"id": 2, "name": "Bob", "phone": "+1234567891"}
    ],
    "groups": [
      {"id": 1, "name": "Team A", "description": "First team", "created_by": 1},
      {"id": 2, "name": "Team B", "description": "Second team", "created_by": 2},
      {"id": 3, "name": "Team C", "description": "Third team", "created_by": 1}
    ]
  },
  "request": {
    "method": "GET",
    "endpoint": "/groups?created_by=1"
  },
  "expected": {
    "status_code": 200,
    "body_contains": ["Team A", "Team C"]
  }
